import snek.file;
import snek.string;


public int[] readInput(string path)
{
	string str = file.readText(path);
	string[] elements = string.split(str, "\n ");
	int[] input = new int[elements.length];
	for (int i = 0; i < elements.length; i++)
		input[i] = int(elements[i]);
	delete str;
	delete elements;
	return input;
}